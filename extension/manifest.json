{
	//
	"manifest_version":2,
	"name":" fblike",
	"description": "đay là tiện ích đầu tiên",
	"version": "1.0",
	"author" : "duyen",
	"icons":{
		"16":"icon.png",
		"48":"icon.png",
		"128":"icon.png"
	},

	"browser_action":{
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	},
	"content_scripts":[
		{
			"matches":["*://*.facebook.com/*"],
			"js":["jquery.min.js","popup.js"]
		}
	]
}